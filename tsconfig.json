{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "esnext",
      "dom"
    ],
    "sourceMap": true,
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "es6",
    "outDir": "./static/"
  }
}